{
  "name": "spoke",
  "version": "1.6.1",
  "description": "Spoke",
  "main": "src/server",
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || ^12.6.0"
  },
  "scripts": {
    "test": "jest --runInBand --forceExit",
    "test-e2e": "jest --runInBand --config jest.config.e2e.js",
    "test-sqlite": "jest --runInBand --config jest.config.sqlite.js",
    "test-coverage": "jest --coverage",
    "test-coverage-bothbackends": "jest --runInBand --config jest.config.sqlite.js -- __test__/backend.test.js &&  jest --coverage",
    "test-knex": "jest --config jest.config.js __test__/server/db/",
    "clean": "rm -rf $OUTPUT_DIR",
    "lint": "eslint --fix --ext js --ext jsx src",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "release": "standard-version",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prod-build-client": "webpack --mode production",
    "prod-build-server": "babel ./src -d ./build/server --source-maps --copy-files",
    "prod-build": "yarn clean && yarn prod-build-client && yarn prod-build-server",
    "prod-static-upload": "if [ \"$S3_STATIC_PATH\" != \"\" ] ; then s3cmd sync --acl-public --guess-mime-type ./build/client/assets/ $S3_STATIC_PATH ; fi",
    "postinstall": "if [ \"$NODE_ENV\" = production ] ; then npm run prod-build && npm run prod-static-upload && npm run notify-slack; fi",
    "notify-slack": "if [ \"$SLACK_NOTIFY_URL\" != \"\" ] ; then echo notifying slack && curl -XPOST -d '{\"channel\": \"#spoke\", \"text\":\"'\"$(whoami) building spoke $(git log -1 --pretty=%h) for $AWS_S3_BUCKET_NAME\"'\"}' $SLACK_NOTIFY_URL; fi",
    "start": "node ./build/server/server",
    "knex": "knex --knexfile ./knexfile.env.js",
    "migrate:worker": "babel-node ./dev-tools/migrate-worker.js",
    "insert-invite": "babel-node ./dev-tools/insert-invite.js",
    "dev-message-sender-01": "nodemon -e js,jsx -w ./src --exec babel-node ./src/workers/message-sender-01",
    "prod-message-sender-01": "babel-node ./src/workers/message-sender-01.js",
    "dev-message-sender-234": "nodemon -e js,jsx -w ./src --exec babel-node ./src/workers/message-sender-234",
    "prod-message-sender-234": "babel-node ./src/workers/message-sender-234.js",
    "dev-message-sender-56": "nodemon -e js,jsx -w ./src --exec babel-node ./src/workers/message-sender-56",
    "prod-message-sender-56": "babel-node ./src/workers/message-sender-56.js",
    "dev-message-sender-789": "nodemon -e js,jsx -w ./src --exec babel-node ./src/workers/message-sender-789",
    "prod-message-sender-789": "babel-node ./src/workers/message-sender-789.js",
    "dev-job-handler": "nodemon -e js,jsx -w ./src --exec babel-node ./src/workers/job-handler",
    "prod-job-handler": "babel-node ./src/workers/job-handler",
    "dev-incoming-message-handler": "nodemon -e js,jsx -w ./src --exec babel-node ./src/workers/incoming-message-handler",
    "prod-incoming-message-handler": "babel-node ./src/workers/incoming-message-handler.js",
    "predev-start": "npm run clean",
    "dev-start-debug": "nodemon -e js,jsx,md,mustache -w ./src --exec ./dev-tools/babel-run-debug  -- ./src/server",
    "generate-contacts": "babel-node ./dev-tools/generate-contacts.js",
    "jest-test": "jest __test__/backend.test.js",
    "dev": "nf start --wrap --procfile ./dev-tools/Procfile.dev",
    "dev:server": "nodemon -e js,jsx,md,mustache -w ./src --exec babel-node ./src/server",
    "dev:client": "webpack-dev-server --config webpack.config.js",
    "dev:client-nowrap": "nf start --trim 500 --procfile ./dev-tools/Procfile.dev",
    "debug-server": "nf start -w --procfile ./dev-tools/Procfile-debug-server.dev",
    "prettier-all": "prettier --write \"src/**/*.js*\" && prettier --write \"dev-tools/**/*.js\" && prettier --write \"deploy/**/*.js?(on)\" && prettier --write \"migrations/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/politics-rewired/spoke.git"
  },
  "keywords": [
    "spoke",
    "sms",
    "texting",
    "campaigns",
    "campaigning",
    "politics",
    "activism",
    "organizing",
    "field"
  ],
